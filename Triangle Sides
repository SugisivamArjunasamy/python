a=int(input("Enter X-coordinate of vertex A:\n"))
b=int(input("Enter Y-coordinate of vertex A:\n"))
c=int(input("Enter X-coordinate of vertex B:\n"))
d=int(input("Enter Y-coordinate of vertex B:\n"))
e=int(input("Enter X-coordinate of vertex C:\n"))
f=int(input("Enter Y-coordinate of vertex C:\n"))
ab=(((c-a)**2)+((d-b)**2))**(1/2)
bc=(((e-c)**2)+((f-d)**2))**(1/2)
ac=(((e-a)**2)+((f-b)**2))**(1/2)
print("Length of sides:")
print("AB - %.1f"%ab)
print("BC - %.1f"%bc)
print("AC - %.1f"%ac)
